<%= simple_form_for [@item, @booking] do |f| %>
  <%= f.input :start_date, as: :string, input_html: {type: :date}, :label => "From:", id: "b_l_1" %>
  <%= f.input :end_date, as: :string, input_html: {type: :date}, :label => "To:", id: "b_l_2" %>
  <%= f.submit 'Confirm now', :class => 'btn btn-primary' %>
<% end %>
